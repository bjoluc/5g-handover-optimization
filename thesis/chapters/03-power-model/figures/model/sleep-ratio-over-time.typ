    yTickStep = 0.5
  }
  if isBottom {
    xLabel = $t space ["ms"]$
  }
  
  canvas({
    draw.set-style(
      axes: (stroke: .5pt, tick: (stroke: .5pt)),
      legend: (stroke: none, spacing: 0.25, item: (spacing: .5em), fill: none)
    )
    plot.plot(
      size: (4.5, 3),
      x-label: xLabel,
      x-tick-step: 40,
      y-label: yLabel,
      y-min: 0,
      y-tick-step: yTickStep,
      y-max: 1,
      {
        plot.add(data, style: (stroke: black))
      }
    )
    draw.content((4.5,2.8), scale(80%)[#align(right)[#legend]], anchor: "north-east")
